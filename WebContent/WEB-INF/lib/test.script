CREATE MEMORY TABLE CARD(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,COLOR INTEGER,IDRFID VARCHAR(255),PLAYER VARCHAR(255),VALUE INTEGER)
CREATE MEMORY TABLE GAME(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,CURRENTMASTER VARCHAR(255),CURRENTTEAMTRUMP VARCHAR(255),CURRENTTRUMP VARCHAR(255),GAMENAME VARCHAR(255),GAMESTATUS VARCHAR(255),TEAM1_SCORE INTEGER NOT NULL,TEAM2_SCORE INTEGER NOT NULL,WINNINGSCORE INTEGER NOT NULL)
CREATE MEMORY TABLE PLAYER(ID BIGINT GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL PRIMARY KEY,NAME VARCHAR(255),TYPE INTEGER)
